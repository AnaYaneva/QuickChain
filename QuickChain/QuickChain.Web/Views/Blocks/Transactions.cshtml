@using QuickChain.Model
@model List<Transaction>
<h2>Transactions</h2>
<div>
    <table id="blocks">
    @for (int i = 0; i < Model.Count; i++)
    {
        var tr = Model[i];
        <tr>
            <td> Tx hash: <a asp-area="" asp-controller="Blocks" asp-action="Transaction" asp-route-id="@tr.TxHash">@tr.TxHash</a></td>
            <td> From: @tr.From</td>
            <td> Time: @tr.To</td>
            <td> Time: @tr.UsedGas</td>
        </tr>

    }
    </table>
</div>